{
    "name": "ehosp/laravel-snomed-ct",
    "description": "A Laravel package for setting up a SNOMED-CT database and terminology server",
    "license": "MIT",
    "authors": [
        {
            "name": "eHOSP",
            "email": "info@ehosp.gr"
        },
        {
            "name": "Alex Karadimos",
            "email": "karadalex@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.4.0|~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpcov": "~3.0|~4.0",
        "phpunit/phpunit": "~5.7.14|~6.1|~7.0",
        "laravel/framework": "~5.4.36|~5.5.0|~5.6.0|~5.7.0",
        "orchestra/testbench": "~3.4.11|~3.5.0|~3.6.0",
        "orchestra/database": "~3.4.0|~3.5.0|~3.6.0",
        "orchestra/testbench-core": "~3.4.5|~3.5.0|~3.6.0",
        "orchestra/testbench-browser-kit": "~3.4.1|~3.5.0|~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "Ehosp\\SnomedCT\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ehosp\\SnomedCT\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Ehosp\\SnomedCT\\SnomedCtServiceProvider"
            ]
        }
    }
}
